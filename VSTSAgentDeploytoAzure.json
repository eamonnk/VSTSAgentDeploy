                    "osDisk": {
                        "createOption": "fromImage",
                        "vhd": {
                            "uri": "[concat(concat(concat('https://', parameters('storageAccountName'), '.blob.core.windows.net/'), 'vhds/'), parameters('virtualMachineName'), '20180912063229.vhd')]"
